+Com ! Kaasaütlev



%>                ! morfeemi piir
%{NA%}            ! nõrk aste
%#		  ! liitsõna piir	

KK1		! kk või k
PP1		! pp või p
TT1		! tt või t

K1		! k või g
B1		! b või null
T1		! t või d
S1		! s või null
G1		! g või null

Definitions

C = [ b|c|d|f|g|h|j|k|l|m|n|p|r|s|t|v ] ;
V = [ a|e|i|o|u|õ|ä|ö|ü ] ;
L = V | C ;

LEXICON Root

Nimisonad ;
Omadussonad ;
Oletused ;

LEXICON Nimisonad

Annika+S: Annika tüüp_annika "weight: 5 " ; 
Randla+S: Randla tüüp_annika ;
Tartu+S: Tartu tüüp_annika ;
kett+S: keTT1 tüüp_kett ;
luba+S: luB1a tüüp_luba "weight: 5 ";
riik+S: riiK1 tüüp_riik ;
süsi+S: süS1 tüüp_süsi ;
vesi+S: veS1 tüüp_vesi ;
krae+S: krae tüüp_krae ;


LEXICON Omadussonad

kena+A: ken tüüp_kena ;
suur+A: suur tüüp_suur ;

LEXICON Oletused

< [V C C] ::10 "+S":0 > tüüp_kett ;        
< [C V C*] ::5 "+S":0 > tüüp_riik ;      
< [C C V V] ::10 "+S":0 > tüüp_krae ;        
< V V C "+S":0 > tüüp_riik ;            
< C V C V "+S":0 > tüüp_luba ;         



LEXICON tüüp_annika

  +Sg+Nom: gi_ki_liide ;
! +Sg+Nom%#: Nimisonad ;
! +Sg+Nom%#: Omadussonad ;
	 :%> sg_gen_jt ;
 +Sg+Part:%>t gi_ki_liide "weight: 1 ";
      +Pl:%>te semkäänded ;
 +Pl+Part:%>id gi_ki_liide ;

LEXICON tüüp_suur
  +Sg+Nom:%> gi_ki_liide ;
! +Sg+Nom%#: Nimisonad ;
! +Sg+Nom%#: Omadussonad ;
	 :%>e sg_gen_jt ;
 +Sg+Part:%>t gi_ki_liide ;
      +Pl:%>te semkäänded ;
 +Pl+Part:%>i gi_ki_liide ;

LEXICON tüüp_kena
  +Sg+Nom:%>a gi_ki_liide ;
! +Sg+Nom%#:%>a Nimisonad ;
! +Sg+Nom%#:%>a Omadussonad ;
         :%{NA%}%>a sg_gen_jt ;
 +Sg+Part:%>a gi_ki_liide ;
      +Pl:%>ade semkäänded ;
+Pl+Part:%>asid gi_ki_liide ;

LEXICON tüüp_kett 
  +Sg+Nom: gi_ki_liide ;
! +Sg+Nom%#: Nimisonad ;
! +Sg+Nom%#: Omadussonad ;
         :%{NA%}i sg_gen_jt ;
 +Sg+Part:%i gi_ki_liide ;
      +Pl:%>ide semkäänded ;
 +Pl+Part:%>e gi_ki_liide ;

LEXICON tüüp_luba
  +Sg+Nom: gi_ki_liide ;
! +Sg+Nom%#: Nimisonad ;
! +Sg+Nom%#: Omadussonad ;
         :%{NA%} sg_gen_jt ;
 +Sg+Part: gi_ki_liide ;
      +Pl:%de semkäänded ;
 +Pl+Part:%sid gi_ki_liide ;

LEXICON tüüp_riik
  +Sg+Nom: gi_ki_liide ;
! +Sg+Nom%#: Nimisonad ;
! +Sg+Nom%#: Omadussonad ;
         :%{NA%}%>i sg_gen_jt ;
 +Sg+Part:%>i gi_ki_liide ;
      +Pl:%>ide semkäänded ;
 +Pl+Part:%>e gi_ki_liide ;

LEXICON tüüp_süsi
  +Sg+Nom:%>i gi_ki_liide ;
! +Sg+Nom%#:%>i Nimisonad ;
! +Sg+Nom%#:%>i Omadussonad ;
         :%{NA%}%>e sg_gen_jt ;
 +Sg+Part:%{NA%}%>tt gi_ki_liide ;
      +Pl:%{NA%}%>te semkäänded ;
 +Pl+Part:%>i gi_ki_liide ;

LEXICON tüüp_vesi
 +Sg+Nom:%>i gi_ki_liide ;
! +Sg+Nom%#:%>i Nimisonad ;
! +Sg+Nom%#:%>i Omadussonad ;
         :%{NA%}%>e sg_gen_jt ;
 +Sg+Part:%{NA%}%>tt gi_ki_liide ;
      +Pl:%{NA%}%>te semkäänded ;
 +Pl+Part:%>i gi_ki_liide ;

LEXICON tüüp_krae
   +Sg+Nom:%> gi_ki_liide ;
! +Sg+Nom%#:%> Nimisonad ;
! +Sg+Nom%#:%> Omadussonad ;
          :%> sg_gen_jt ;
  +Sg+Part:%>d gi_ki_liide ;
       +Pl:%>d semkäänded ;
  +Pl+Part:%>esid gi_ki_liide ;

LEXICON sg_gen_jt

         +Sg+Gen: gi_ki_liide ;
!       +Sg+Gen:%# Nimisonad ;
!       +Sg+Gen:%# Omadussonad ;
             +Sg: semkäänded ;
         +Pl+Nom:%>d gi_ki_liide ;

LEXICON 2C

  : sg_gen_jt ; 
  +Sg+Par:%>t gi_ki_liide ;
  +Pl:%>te semkäänded ;
  +Pl+Par:%>id gi_ki_liide ;

LEXICON semkäänded

	+Gen: gi_ki_liide ;
	+Ill: %>sse gi_ki_liide ;
	+Ine: %>s gi_ki_liide ;
	+Ela: %>st gi_ki_liide ;
	+All: %>le gi_ki_liide ;
	+Ade: %>l gi_ki_liide ;
	+Abl: %>lt gi_ki_liide ;
	+Trl: %>ks gi_ki_liide ;
	+Ter: %>ni gi_ki_liide ;
	+Ess: %>na gi_ki_liide ;
	+Abe: %>ta gi_ki_liide ;
	+Com: %>ga gi_ki_liide ;

LEXICON  gi_ki_liide
	 +gi_ki_liide:%>gi # ;
	 : # ; ! kui gi või ki puudub

